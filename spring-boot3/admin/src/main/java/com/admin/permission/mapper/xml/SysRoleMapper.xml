<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.admin.permission.mapper.SysRoleMapper">
    <select id="selectAll" resultType="com.admin.permission.response.SysRoleResponse">
        SELECT id, name, code, description
        FROM sys_role
        WHERE status = 1
    </select>
</mapper>